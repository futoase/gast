%h1
  Items
%table{ class: "table table-striped" }
  %thead
    %tr 
      %th
        Content ID
      %th
        Updated At
  %tbody
    - @lists.each do |list|
      %tr
        %td
          %a{ href: url("/posts/view/#{list[:content_id]}") }
            = list[:content_id]
        %td
          = list[:updated_at]
